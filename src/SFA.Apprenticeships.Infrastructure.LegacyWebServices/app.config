<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IReferenceData">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpBinding_IReferenceData1" />
                <binding name="BasicHttpBinding_IVacancySummary">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpBinding_IVacancySummary1" />
