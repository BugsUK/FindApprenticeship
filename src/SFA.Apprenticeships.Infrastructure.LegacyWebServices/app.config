                contract="ReferenceDataProxy.IReferenceData" name="BasicHttpBinding_IReferenceData" />
            <endpoint address="https://apprenticeshipvacancymatchingservice.lsc.gov.uk/Services/VacancySummary/VacancySummary51.svc"
