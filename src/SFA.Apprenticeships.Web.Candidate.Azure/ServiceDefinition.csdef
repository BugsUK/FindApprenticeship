<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="SFA.Apprenticeships.Web.Candidate.Azure" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2014-06.2.4">
  <WebRole name="SFA.Apprenticeships.Web.Candidate" vmsize="Small">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="CandidateWebEndpoint" endpointName="CandidateWebEndpoint" />
        </Bindings>
      </Site>
    </Sites>
    <Endpoints>
      <InputEndpoint name="CandidateWebEndpoint" protocol="http" port="80" />
    </Endpoints>
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <Certificates>
      <Certificate name="NasClientCertificate" storeLocation="LocalMachine" storeName="My" />
    </Certificates>
    <Startup>
      <Task commandLine="Startup\startup.cmd" executionContext="elevated" />
    </Startup>	
  </WebRole>
</ServiceDefinition>