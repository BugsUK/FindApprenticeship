@model SFA.Apprenticeships.Web.Raa.Common.ViewModels.Application.AboutYouViewModel

<fieldset class="fieldset-with-border">
    <legend class="heading-large">
        About you
    </legend>
    <div class="form-group">
        <label for="strengths" class="form-label">What are your main strengths?</label>
        <span class="form-prepopped" id="strengths">@Model.Strengths</span>
    </div>
    <div class="form-group">
        <label for="improvements" class="form-label">What skills would you like to improve during this apprenticeship?</label>
        <span class="form-prepopped" id="improvements">@Model.Improvements</span>
    </div>
    <div class="form-group">
        <label for="hobbies-and-interests" class="form-label">What are your hobbies and interests?</label>
        <span class="form-prepopped" id="hobbies-and-interests">@Model.HobbiesAndInterests</span>
    </div>
</fieldset>