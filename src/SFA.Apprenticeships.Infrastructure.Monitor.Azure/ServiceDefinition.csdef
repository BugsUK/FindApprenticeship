<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="SFA.Apprenticeships.Infrastructure.Monitor.Azure" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2014-06.2.4">
  <WorkerRole name="SFA.Apprenticeships.Infrastructure.Monitor" vmsize="Small">
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="StorageConnectionString" />
      <Setting name="QueueName" />
    </ConfigurationSettings>
    <Certificates>
      <Certificate name="NasClientCertificate" storeLocation="LocalMachine" storeName="My" />
    </Certificates>
  </WorkerRole>
</ServiceDefinition>