<?xml version="1.0"?>
<!-- For more information on using app.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="RunVacancyNotFoundTest" value="false" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
  </appSettings>
  <specBind>
    <application startUrl="http://local.candidates.gov.uk/" xdt:Transform="Replace"/>
    <browserFactory browserType="FireFox" elementLocateTimeout="00:00:03" driverNeedsValidation="true" xdt:Transform="SetAttributes"/>
  </specBind>
</configuration>