      <xsd:complexType name="BSaddressStructure">
        <xsd:sequence>
          <xsd:element minOccurs="0" name="SAON" type="s2:AONstructure">
            <xsd:annotation>
              <xsd:documentation>Secondary Addressable Object</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PAON" type="s2:AONstructure">
            <xsd:annotation>
              <xsd:documentation>Primary Addressable Object</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:sequence>
            <xsd:element name="StreetDescription" type="s2:StreetDescriptorType" />
            <xsd:element minOccurs="0" name="UniqueStreetReferenceNumber" type="s2:USRNtype" />
          </xsd:sequence>
          <xsd:choice>
            <xsd:sequence>
              <xsd:element ref="s2:Locality" />
              <xsd:choice minOccurs="0">
                <xsd:sequence>
                  <xsd:element ref="s2:Town" />
                  <xsd:element minOccurs="0" ref="s2:AdministrativeArea" />
                </xsd:sequence>
                <xsd:element ref="s2:AdministrativeArea" />
              </xsd:choice>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:element ref="s2:Town" />
              <xsd:element minOccurs="0" ref="s2:AdministrativeArea" />
            </xsd:sequence>
            <xsd:element ref="s2:AdministrativeArea" />
          </xsd:choice>
          <xsd:element minOccurs="0" name="PostTown" type="s2:PostTownType" />
          <xsd:element minOccurs="0" name="PostCode" type="s2:PostCodeType" />
          <xsd:element minOccurs="0" name="UniquePropertyReferenceNumber" type="s2:UPRNtype" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:simpleType name="UPRNtype">
        <xsd:restriction base="xsd:integer">
          <xsd:minInclusive value="1" />
          <xsd:maxInclusive value="999999999999" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="USRNtype">
        <xsd:restriction base="xsd:integer">
          <xsd:minInclusive value="1" />
          <xsd:maxInclusive value="99999999" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="PostCodeType">
        <xsd:annotation>
          <xsd:documentation>complex pattern for postcode, which matches definition, accepted by some parsers is: "(GIR 0AA)|((([A-Z-[QVX]][0-9][0-9]?)|(([A-Z-[QVX]][A-Z-[IJZ]][0-9][0-9]?)|(([A-Z-[QVX]][0-9][A-HJKSTUW])|([A-Z-[QVX]][A-Z-[IJZ]][0-9][ABEHMNPRVWXY])))) [0-9][A-Z-[CIKMOV]]{2})"</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
          <xsd:pattern value="[A-Z]{1,2}[0-9R][0-9A-Z]? [0-9][A-Z-[CIKMOV]]{2}" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="PostTownType">
        <xsd:restriction base="xsd:string">
          <xsd:minLength value="1" />
          <xsd:maxLength value="30" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="LocalityType">
        <xsd:restriction base="xsd:string">
          <xsd:minLength value="1" />
          <xsd:maxLength value="35" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="AdministrativeAreaType">
        <xsd:restriction base="xsd:string">
          <xsd:minLength value="1" />
          <xsd:maxLength value="30" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="TownType">
        <xsd:restriction base="xsd:string">
          <xsd:minLength value="1" />
          <xsd:maxLength value="30" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="StreetDescriptorType">
        <xsd:restriction base="xsd:string">
          <xsd:minLength value="1" />
          <xsd:maxLength value="100" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="StreetReferenceTypeType">
        <xsd:restriction base="xsd:integer">
          <xsd:enumeration value="1" />
          <xsd:enumeration value="2" />
          <xsd:enumeration value="3" />
          <xsd:enumeration value="4" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="VersionNumberType">
        <xsd:restriction base="xsd:positiveInteger">
          <xsd:minInclusive value="1" />
          <xsd:maxInclusive value="9999" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="ToleranceType">
        <xsd:restriction base="xsd:decimal">
          <xsd:minInclusive value="0" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="RefPointGridCoordinateType">
        <xsd:restriction base="xsd:string">
          <xsd:pattern value="[0-9]{14}" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="StreetStructure">
        <xsd:sequence>
          <xsd:element name="StreetReferenceType" type="s2:StreetReferenceTypeType" />
          <xsd:element name="StartCoordinate" type="s2:CoordinateStructure" />
          <xsd:element name="EndCoordinate" type="s2:CoordinateStructure" />
          <xsd:element name="Tolerance" type="s2:ToleranceType" />
          <xsd:element name="StreetVersionNumber" type="s2:VersionNumberType" />
          <xsd:element name="StreetEntryDate" type="xsd:date" />
          <xsd:element minOccurs="0" name="StreetClosureDate" type="xsd:date" />
          <xsd:element name="ResponsibleAuthority" type="s2:CustodianCodeType" />
          <xsd:element name="DescriptiveIdentifier" type="s2:StreetDescriptiveIdentifierStructure" />
          <xsd:element minOccurs="0" name="StreetAlias" type="s2:StreetDescriptiveIdentifierStructure" />
          <xsd:element minOccurs="0" name="StreetCrossReferences">
            <xsd:complexType>
              <xsd:choice>
                <xsd:element name="UniqueStreetReferenceNumbers" type="s2:USRNlistType" />
                <xsd:element minOccurs="0" maxOccurs="unbounded" name="ElementaryStreetUnit" type="s2:ElementaryStreetUnitStructure" />
              </xsd:choice>
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="usrn" type="s2:USRNtype" />
      </xsd:complexType>
      <xsd:complexType name="StreetDescriptiveIdentifierStructure">
        <xsd:sequence>
          <xsd:element name="StreetDescription" type="s2:StreetDescriptorType" />
          <xsd:choice>
            <xsd:sequence>
              <xsd:element ref="s2:Locality" />
              <xsd:choice minOccurs="0">
                <xsd:sequence>
                  <xsd:element ref="s2:Town" />
                  <xsd:element minOccurs="0" ref="s2:AdministrativeArea" />
                </xsd:sequence>
                <xsd:element ref="s2:AdministrativeArea" />
              </xsd:choice>
            </xsd:sequence>
            <xsd:sequence>
              <xsd:element ref="s2:Town" />
              <xsd:element minOccurs="0" ref="s2:AdministrativeArea" />
            </xsd:sequence>
            <xsd:element ref="s2:AdministrativeArea" />
          </xsd:choice>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="ElementaryStreetUnitStructure">
        <xsd:sequence>
          <xsd:element name="ESUidentity" type="s2:RefPointGridCoordinateType" />
          <xsd:element name="ESUversion" type="s2:VersionNumberType" />
          <xsd:element name="ESUentryDate" type="xsd:date" />
          <xsd:element minOccurs="0" name="ESUclosureDate" type="xsd:date" />
          <xsd:element name="StartCoordinate" type="s2:CoordinateStructure" />
          <xsd:element name="EndCoordinate" type="s2:CoordinateStructure" />
          <xsd:element name="Tolerance" type="s2:ToleranceType" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="IntermediateCoord" type="s2:CoordinateStructure" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="CoordinateStructure">
        <xsd:annotation>
          <xsd:documentation>Coordinate Point based on UK National Grid</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="X">
            <xsd:annotation>
              <xsd:documentation>Easting Field</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:unsignedLong">
                <xsd:minInclusive value="0" />
                <xsd:maxInclusive value="9999999" />
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="Y">
            <xsd:annotation>
              <xsd:documentation>Northing Field</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:unsignedLong">
                <xsd:minInclusive value="0" />
                <xsd:maxInclusive value="9999999" />
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:simpleType name="UPRNlistType">
        <xsd:list itemType="s2:UPRNtype" />
      </xsd:simpleType>
      <xsd:simpleType name="USRNlistType">
        <xsd:list itemType="s2:USRNtype" />
      </xsd:simpleType>
      <xsd:complexType name="BLPUpolygonStructure">
        <xsd:sequence>
          <xsd:element name="PolygonID">
            <xsd:simpleType>
              <xsd:restriction base="xsd:positiveInteger">
                <xsd:minInclusive value="1" />
                <xsd:maxInclusive value="99" />
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element minOccurs="0" name="PolygonType">
            <xsd:annotation>
              <xsd:documentation>H to represent a hole or not present</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:enumeration value="H" />
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:choice>
            <xsd:element minOccurs="3" maxOccurs="unbounded" name="Vertices" type="s2:CoordinateStructure" />
            <xsd:element name="ExternalRef" type="xsd:unsignedLong" />
          </xsd:choice>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="BLPUextentStructure">
        <xsd:sequence>
          <xsd:element name="SourceDescription">
            <xsd:annotation>
              <xsd:documentation>Description of Source of BLPU Extent</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:minLength value="1" />
                <xsd:maxLength value="50" />
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="ExtentEntryDate" type="xsd:date" />
          <xsd:element name="ExtentSourceDate" type="xsd:date" />
          <xsd:element name="ExtentStartDate" type="xsd:date" />
          <xsd:element minOccurs="0" name="ExtentEndDate" type="xsd:date" />
          <xsd:element name="ExtentLastUpdateDate" type="xsd:date" />
          <xsd:element maxOccurs="unbounded" name="ExtentDefinition" type="s2:BLPUpolygonStructure" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:simpleType name="ProvenanceCodeType">
        <xsd:annotation>
          <xsd:documentation>Provenance Code values
					T = Registered land title
					L = Unregistered land title
					F = Formal tenancy agreement
					R = Rental agreement
					P = Physical features
					O = Occupancy
					U = Use</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="T" />
          <xsd:enumeration value="L" />
          <xsd:enumeration value="F" />
          <xsd:enumeration value="R" />
          <xsd:enumeration value="P" />
          <xsd:enumeration value="O" />
          <xsd:enumeration value="U" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="ProvenanceStructure">
        <xsd:sequence>
          <xsd:element name="ProvenanceCode" type="s2:ProvenanceCodeType" />
          <xsd:element minOccurs="0" name="Annotation" type="xsd:string" />
          <xsd:element name="ProvEntryDate" type="xsd:date" />
          <xsd:element name="ProvStartDate" type="xsd:date" />
          <xsd:element minOccurs="0" name="ProvEndDate" type="xsd:date" />
          <xsd:element name="ProvLastUpdateDate" type="xsd:date" />
          <xsd:element minOccurs="0" name="BLPUextent" type="s2:BLPUextentStructure" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:simpleType name="CustodianCodeType">
        <xsd:restriction base="xsd:integer">
          <xsd:minInclusive value="1" />
          <xsd:maxInclusive value="9999" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="RepresentativePointCodeType">
        <xsd:annotation>
          <xsd:documentation>Representative Point Code values
					1 = Visual centre
					2 = General internal point
					3 = South-west corner of 100m grid square
					4 = Start point of the referenced street</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:positiveInteger">
          <xsd:enumeration value="1" />
          <xsd:enumeration value="2" />
          <xsd:enumeration value="3" />
          <xsd:enumeration value="4" />
          <xsd:enumeration value="5" />
          <xsd:enumeration value="9" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="LogicalStatusType">
        <xsd:annotation>
          <xsd:documentation>Logical Status values
					1 = Approved preferred
					2 = Approved alternative (LPI Only)
					3 = Alternative (LPI Only)
					5 = Candidate
					6 = Provisional
					8 = Historical
					9 = Rejected</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:integer">
          <xsd:minInclusive value="1" />
          <xsd:maxInclusive value="9" />
          <xsd:enumeration value="1" />
          <xsd:enumeration value="2" />
          <xsd:enumeration value="3" />
          <xsd:enumeration value="5" />
          <xsd:enumeration value="6" />
          <xsd:enumeration value="8" />
          <xsd:enumeration value="9" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="BasicLandAndPropertyUnitStructure">
        <xsd:sequence>
          <xsd:element ref="s2:UniquePropertyReferenceNumber" />
          <xsd:element name="CustodianCode" type="s2:CustodianCodeType" />
          <xsd:element name="RepresentativePointCode" type="s2:RepresentativePointCodeType" />
          <xsd:element name="LogicalStatus" type="s2:LogicalStatusType" />
          <xsd:element name="GridReference" type="s2:CoordinateStructure" />
          <xsd:element name="BLPUentryDate" type="xsd:date" />
          <xsd:element name="BLPUstartDate" type="xsd:date" />
          <xsd:element minOccurs="0" name="BLPUendDate" type="xsd:date" />
          <xsd:element name="BLPUlastUpdateDate" type="xsd:date" />
          <xsd:element maxOccurs="unbounded" name="LandAndPropertyIdentifier" type="s2:LandAndPropertyIdentifierStructure" />
          <xsd:element maxOccurs="unbounded" name="Provenance" type="s2:ProvenanceStructure" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="LandAndPropertyIdentifierStructure">
        <xsd:sequence>
          <xsd:element name="PAON" type="s2:AONstructure" />
          <xsd:element minOccurs="0" name="SAON" type="s2:AONstructure" />
          <xsd:element minOccurs="0" name="PostTown" type="s2:PostTownType" />
          <xsd:element minOccurs="0" name="PostCode" type="s2:PostCodeType" />
          <xsd:element minOccurs="0" name="Level">
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:minLength value="1" />
                <xsd:maxLength value="30" />
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="LogicalStatus" type="s2:LogicalStatusType" />
          <xsd:element minOccurs="0" name="OfficialAddressMarker" type="xsd:boolean" />
          <xsd:element name="LPIstartDate" type="xsd:date" />
          <xsd:element name="LPIentryDate" type="xsd:date" />
          <xsd:element minOccurs="0" name="LPIendDate" type="xsd:date" />
          <xsd:element name="LPIlastUpdateDate" type="xsd:date" />
          <xsd:choice>
            <xsd:element name="Street" type="s2:StreetStructure" />
            <xsd:element name="USRN" type="s2:USRNtype" />
          </xsd:choice>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="Town" type="s2:TownType" />
      <xsd:element name="Locality" type="s2:LocalityType" />
      <xsd:element name="AdministrativeArea" type="s2:AdministrativeAreaType" />
      <xsd:element name="UniquePropertyReferenceNumber" type="s2:UPRNtype" />
      <xsd:element name="BS7666Address" type="s2:BSaddressStructure" />
      <xsd:element name="BS7666BLPU" type="s2:BasicLandAndPropertyUnitStructure" />
      <xsd:simpleType name="AONdescriptionType">
        <xsd:annotation>
          <xsd:documentation>Restricted string type for the name text portion of the PAON and SAON</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
          <xsd:minLength value="1" />
          <xsd:maxLength value="90" />
          <xsd:pattern value="[a-zA-Z0-9:;&lt;=&gt;\?@%&amp;'\(\)\*\+,\-\. ]{0,90}" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="AONrangeStructure">
        <xsd:annotation>
          <xsd:documentation>Numeric and optional alpha suffix for start and end numbers</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="Number">
            <xsd:simpleType>
              <xsd:restriction base="xsd:positiveInteger">
                <xsd:maxInclusive value="9999" />
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element minOccurs="0" name="Suffix">
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:minLength value="1" />
                <xsd:maxLength value="1" />
                <xsd:pattern value="[A-Z]" />
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="AONstructure">
        <xsd:annotation>
          <xsd:documentation>Replacement for PAONtype and SAONtype</xsd:documentation>
        </xsd:annotation>
        <xsd:choice>
          <xsd:sequence>
            <xsd:element name="StartRange" type="s2:AONrangeStructure" />
            <xsd:element minOccurs="0" name="EndRange" type="s2:AONrangeStructure" />
            <xsd:element minOccurs="0" name="Description" type="s2:AONdescriptionType" />
          </xsd:sequence>
          <xsd:element name="Description" type="s2:AONdescriptionType" />
        </xsd:choice>
      </xsd:complexType>
